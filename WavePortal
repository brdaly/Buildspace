pragma solidity ^0.8.4;

import "hardhat/console.sol";

contract MyEthProject {
    uint totalWaves;

    constructor() {
        console.log("brdaly wassup!!");
    }
    
    // POST Function
    function wave() public {
        totalWaves += 1;
        console.log("Just got a wave! totalWaves = %s", totalWaves);
    }
    // GET Function
    function getTotalWaves() public view returns (uint) {
        return totalWaves;
    }
}
